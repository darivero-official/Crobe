        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Controlling object
y_offset = global.dvc_height;
height = max(global.dvc_width, global.dvc_height)*0.35;

if to != 0 {
    fase += to * 0.04;
    ease_fase = keyboard_get_easing(fase, -3*to)
    y_offset = global.dvc_height+( height * (1-ease_fase));
}

if fase &gt; 1 &amp;&amp; to == 1 {
    to = 0;
    fase = 1;
    ease_fase = 1;
    y_offset = global.dvc_height+( height * (1-ease_fase));
}
if fase &lt; 0 &amp;&amp; to == -1 {
    to = 0;
    fase = 0;
    ease_fase = 0;
    y_offset = global.dvc_height+( height * (1-ease_fase));
}

if key_go==2 {
    if instance_exists(objPop_auth) {
        if objPop_auth.v_msg_input_sel = 1 {
            objPop_auth.v_msg_input_1txt = keyboard_string;
        }
        else if objPop_auth.v_msg_input_sel = 2 {
            objPop_auth.v_msg_input_2txt = keyboard_string;
        }
        else if objPop_auth.v_msg_input_sel = 3 {
            objPop_auth.v_msg_input_3txt = keyboard_string;
        }
        else if objPop_auth.v_msg_input_sel = 4 {
            objPop_auth.v_msg_input_4txt = keyboard_string;
        }
    }
    global.keyboard=-1;
    instance_destroy();
}

if keyboard_check_released(vk_backspace) &amp;&amp; (os_type = os_android || os_type = os_ios) { //If back button is released
    //to = -1; //Disable the keyboard
    instance_destroy();
}
